DEFAULT install
LABEL install
    KERNEL vmlinuz
    INITRD initrd
    APPEND ip=dhcp ramdisk_size=1500000 url=http://$IP_HOST/ubuntu-20.04.3-live-server-amd64.iso splash=verbose autoinstall console=ttyS0,115200u8 ds=nocloud-net;s=http:$IP_HOST//host:8000/cloud-init-bios/ root=/dev/ram0 cloud-config-url=http://IP_HOST:8000/cloud-init-bios/user-data/$user-data
